  <header class="bg-primary navbar navbar-dark">
    <a class="navbar-brand text-left" href="#"><%= projectName %>
      <i class="icon ion-md-square-outline text-white mr-1"></i><i class="icon ion-md-checkbox-outline text-green mr-1"></i><i class="icon ion-md-square-outline text-white mr-1"></i>
    </a>
  </header>
  <nav class="navbar navbar-expand-md navbar-dark bg-primary">


      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <% if (locals.user != null) { %>
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
            <a class="nav-link" href="/dashboard">Dashboard <span class="sr-only">(current)</span></a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/projects">Projects</a>
          </li>
          <li class="nav-item">
            <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a>
          </li>
        </ul>
        <% } %>
        <% if (locals.user == null) { %>
        <form class="form-inline my-2 my-lg-0" action="/users/login" method="POST">
          <input class="form-control mr-sm-2" type="email" placeholder="Email" aria-label="Email" name="email">
          <input class="form-control mr-sm-2" type="password" placeholder="Password" aria-label="Password" name="password">
          <button class="btn btn-outline-warning my-2 my-sm-0" type="submit">LogIn</button>
        </form>
        <a href="/users/register" class="ml-2 btn btn-outline-warning">Register</a>

      <% } else { %>

        <span class="nav-item dropdown">
          <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            <i class="icon ion-md-contact text-light"></i>
          </a>
          <div class="dropdown-menu" aria-labelledby="navbarDropdown">
            <a class="dropdown-item" href="/users/me">Profile</a>
            <a class="dropdown-item" href="/users/logout">LogOut</a>
          </div>
        </span>
      <% } %>
    </div>
  </nav>
